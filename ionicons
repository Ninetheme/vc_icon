
	// Add new custom font to Font Family selection in icon box module
	function bon_add_ionicons( ) {
		$param = WPBMap::getParam( 'vc_icon', 'type' );
		$param['value'][__( 'Ionicons', 'bon' )] = 'ionicons';
		vc_update_shortcode_param( 'vc_icon', $param );
	}
	add_filter( 'init', 'bon_add_ionicons', 40 );

	// Add font picker setting to icon box module when you select your font family from the dropdown
	function bon_add_font_picker() {
		vc_add_param( 'vc_icon', array(
				'type' 			=> 'iconpicker',
				'weight' 		=> 1,
				'heading' 		=> esc_html__( 'Icon', 'bon' ),
				'param_name' 	=> 'icon_ionicons',
				'settings' 		=> array(
						'emptyIcon' 	=> false,
						'type' 			=> 'ionicons',
						'iconsPerPage' 	=> 200,
				),
				'dependency' 	=> array(
						'element' 	=> 'type',
						'value' 	=> 'ionicons',
				),
			)
		);
	}
	add_filter( 'vc_after_init', 'bon_add_font_picker', 40 );

